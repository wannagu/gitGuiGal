{
  "name": "yuki",
  "version": "0.1.0",
  "author": "tinyAdapter <tinyadapter@outlook.com>",
  "description": "Yummy Utterance Knowledge Interface",
  "license": "GPL-3.0",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "dev": "chcp 65001 && node .electron-vue/dev-runner.js",
    "e2e": "npm run pack && mocha test/e2e",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "test": "npm run unit && npm run e2e",
    "unit:main": "cross-env NODE_ENV=none karma start test/unit/karma.main.conf.js",
    "unit:renderer": "cross-env NODE_ENV=none karma start test/unit/karma.renderer.conf.js",
    "rebuild": "electron-builder install-app-deps",
    "postinstall": "npm run rebuild",
    "tsc": "tsc --noEmit"
  },
  "build": {
    "productName": "YUKI",
    "appId": "org.project-yuki.yuki",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*",
      "build/icons/*"
    ],
    "extraFiles": [
      "dict/**/*",
      "lib/**/*",
      "config/youdaoApi.js",
      "config/hjdictApi.js",
      "config/qqApi.js"
    ],
    "win": {
      "icon": "build/icons/icon.png"
    }
  },
  "config": {
    "arch": "ia32"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "debug": "^4.1.1",
    "electron-vibrancy": "^0.1.3",
    "extract-zip": "^1.6.7",
    "ffi": "^2.3.0",
    "fstream": "^1.0.12",
    "jsonfile": "^5.0.0",
    "mecab-ffi": "^0.2.0",
    "ref": "^1.3.5",
    "request": "^2.88.0",
    "request-progress": "^3.0.0",
    "request-promise-native": "^1.0.5",
    "sqlite3": "^4.1.0",
    "tasklist": "^3.1.1",
    "textractor-wrapper": "^2.0.1",
    "vue": "^2.6.10",
    "vue-color": "^2.7.0",
    "vue-electron": "^1.0.6",
    "vue-i18n": "^8.11.2",
    "vue-router": "^3.0.1",
    "vuetify": "^2.6.10",
    "vuetify-dialog": "^1.0.0-alpha.5",
    "vuex": "^3.0.1",
    "wanakana": "^4.0.2",
    "xml2js": "^0.4.22",
    "xterm": "^4.2.0-vscode1",
    "xterm-addon-fit": "^0.3.0"
  },
